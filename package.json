{
  "name": "wld-id-tst",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.cjs",
  "module": "dist/module.js",
  "iife": "dist/world-id.js",
  "types": "dist/types.d.ts",
  "react": "dist/react/index.js",
  "react-types": "dist/react/index.d.ts",
  "utils": "dist/utils/index.js",
  "utils-types": "dist/utils/index.d.ts",
  "targets": {
    "iife": {
      "context": "browser",
      "source": "src/browser/index.tsx",
      "includeNodeModules": true,
      "outputFormat": "global"
    },
    "main": {
      "source": "src/widget/index.ts"
    },
    "module": {
      "source": "src/widget/index.ts"
    },
    "types": {
      "source": "src/widget/index.ts"
    },
    "react": {
      "source": "src/components/index.ts",
      "outputFormat": "esmodule",
      "isLibrary": true
    },
    "react-types": {
      "source": "src/components/index.ts"
    },
    "utils": {
      "source": "src/utils/index.ts",
      "outputFormat": "esmodule",
      "isLibrary": true
    },
    "utils-types": {
      "source": "src/utils/index.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "alias": {
    "components": "./src/components",
    "widget": "./src/widget"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && parcel build",
    "dev": "parcel watch -p 3000",
    "typecheck": "tsc --noEmit",
    "rebuild": "npm run build && git add . && git commit -m \"rebuild\" && git push"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.6.2",
    "@parcel/transformer-typescript-tsc": "^2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "parcel": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorosip0v/wld-id-tst.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/igorosip0v/wld-id-tst/issues"
  },
  "homepage": "https://github.com/igorosip0v/wld-id-tst#readme"
}
